# Configurações do Banco de Dados
DATABASE_URL=jdbc:postgresql://localhost:5432/nordeste-servicos
DATABASE_USERNAME=postgres
DATABASE_PASSWORD=123

# Configurações do JWT
JWT_SECRET=sua-chave-secreta-super-longa-e-segura-aqui-com-pelo-menos-256-bits
JWT_EXPIRATION=36000000

# Configurações de CORS
CORS_ALLOWED_ORIGINS=http://localhost:3000,http://localhost:5173

# Configurações da Aplicação - OTIMIZADAS PARA ECONOMIA
SPRING_PROFILES_ACTIVE=prod
SHOW_SQL=false
PORT=8080

# Configurações de Memória Java - OTIMIZADAS
JAVA_OPTS=-Xmx256m -Xms128m -XX:+UseG1GC -XX:MaxGCPauseMillis=200 -XX:+UseStringDeduplication

# Configurações de Logging - MÍNIMO
LOGGING_LEVEL_ROOT=ERROR
LOGGING_LEVEL_COM_CODAGIS=WARN

# Configurações do Banco - POOL MÍNIMO
SPRING_DATASOURCE_HIKARI_MAXIMUM_POOL_SIZE=3
SPRING_DATASOURCE_HIKARI_MINIMUM_IDLE=1 